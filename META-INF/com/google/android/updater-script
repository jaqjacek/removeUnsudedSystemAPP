ui_print("Jaq remove unused system app");
ui_print("                 Device: Moto OMAP4");
ui_print("      Build time: 05.05.2017");
ui_print(" ");
show_progress(0.500000, 3);
mount("ext4", "EMMC", "/dev/block/mmcblk1p20", "/system");
delete_recursive("/system/app/BasicDreams");
ui_print("Remove BlueTooth scripting ");
delete_recursive("/system/app/Bluetooth");
ui_print("Remove Browser ");
delete_recursive("/system/app/Browser");
ui_print("Remove Calendar ");
delete_recursive("/system/app/Calendar");
delete_recursive("/system/app/EasterEgg");
ui_print("Remove Email");
delete_recursive("/system/app/Email");
delete_recursive("/system/app/Exchange2");
delete_recursive("/system/app/HTMLViewer");
ui_print("Remove Email");
delete_recursive("/system/app/LiveWallpapersPicker");
delete_recursive("/system/app/Profiles");
ui_print("Remove Priv-apps");
delete_recursive("/system/priv-app/CMUpdater");
delete_recursive("/system/priv-app/CellBroadcastReceiver");
delete_recursive("/system/priv-app/LineageSetupWizard");
delete_recursive("/system/priv-app/MotoDoze");
delete_recursive("/system/priv-app/CMParts");
delete_recursive("/system/priv-app/LiveLockScreenService");
delete_recursive("/system/priv-app/WeatherProvider");
delete_recursive("/system/priv-app/WeatherManagerService");
delete_recursive("/system/priv-app/WallpaperCropper");
show_progress(0.500000, 0);
unmount("/system");
ui_print("Completed. Reboot.");

